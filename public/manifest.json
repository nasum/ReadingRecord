{
  "manifest_version": 3,
  "name": "ReadingRecord",
  "description": "recorad website",
  "version": "0.0.1",
  "icons": { "128": "image/icon128.png" },
  "permissions": ["contextMenus", "tabs"],
  "content_security_policy": {
      "extension_pages": "script-src *; object-src 'self'"
  },
  "browser_action": {
    "default_popup": "./default_popup.html"
  }
}
